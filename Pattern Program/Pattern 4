// Print pattern
// 123
// 456
// 789

#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int val=0;
    int i=1;
    while(i<=n){
        int j=1;
        while(j<=n){
            val=val+1;
            cout<<val;
            j++;
        }
        cout<<endl;
        i++;
    }
}
