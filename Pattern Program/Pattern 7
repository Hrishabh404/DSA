// Print pattern
// 1
// 23
// 456
// 78910
#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int val=0,i=1;
    while(i<=n){
        int j=1;
        while(j<=i){
            val=val+1;
            cout<<val;
            j++;
        }
        cout<<endl;
        i++;
    }
}
